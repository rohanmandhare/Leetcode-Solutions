import java.util.*;
import java.util.Scanner;

public class Main{
  public static void main(String[] args){
    Scanner input = new Scanner(System.in);
    int n = input.nextInt();
    int[] arr = new int[n];
    for (int i=0 ; i<n ; i++){
      arr[k] = input.nextInt();
    }
    for (int i=0 ; i<n ; i++){
      for (int j=i ; j<n ; j++){
        for (int k=i ; k<=j ; k++){
         System.out.print(arr[k]);
         }
         System.out.println();
      }
    }
    
    
  }
}
